    "git.openRepositoryInParentFolders": "never",
    "editor.semanticHighlighting.enabled": true,
    "typst-lsp.serverPath": "",
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["<leader>", "f", "f"],
            "commands": [
                "workbench.action.quickOpen"
            ]
        },
        {
            "before": [
                "<C-u>"
            ],
            "after": [
                "<C-u>", "z", "z"
            ]
        },
        {
            "before": [
                "<C-d>"
            ],
            "after": [
                "<C-d>", "z", "z"
            ]
        },
        {
            "before": [
                "<"
            ],
            "after": [
                "<", "<"
            ]
        },
        {
            "before": [
                ">"
            ],
            "after": [
                ">", ">"
            ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<"
            ],
            "after": [
                "<", "<", "<Esc>", "g", "v"
            ]
        },
        {
            "before": [
                ">"
            ],
            "after": [
                ">", ">", "<Esc>", "g", "v"
            ]
        },
        {
            "before": [
                "R"
            ],
            "after": [
                "\"", "_", "d", "P"
            ]
        }
    ],
    "vim.leader": " ",
    "zig.zls.semanticTokens": "none",
    "dart.flutterSdkPath": "E:\\dev\\repos\\flutter",
    "cmake.showOptionsMovedNotification": false,
    "zig.zls.enableInlayHints": false,
    "window.commandCenter": false,
    "window.menuBarVisibility": "compact",
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer"
    },
    "vim.sneak": true,
    "vim.highlightedyank.enable": true,
    "vim.highlightedyank.color": "rgba(39, 163, 5, 0.3)",
    "cmake.pinnedCommands": [
        "workbench.action.tasks.configureTaskRunner",
        "workbench.action.tasks.runTask"
    ],
    "zig.zls.enabled": "on",
    "ols.prompt.AskCreateOLS": false,
    "redhat.telemetry.enabled": false,
    "codetogether.userName": "Olibub",
    "haskell.manageHLS": "GHCup",
    "editor.matchBrackets": "near",
    "print.folder.fileNames": true,
    "print.folder.include": [],
    "print.folder.exclude": [
        "{bin,obj,out}",
        "node_modules",
        "data",
        "**/*.{bin,exe,dll,hex,pdb,pdf,pfx,png,jpg,gif,bmp,suo,design}"
    ],
    "print.folder.maxLines": 1200,
    "print.folder.maxFiles": 30,
    "print.folder.includeFileList": false,
    "go.showWelcome": false,
    "workbench.welcomePage.walkthroughs.openOnInstall": false,
    "workbench.startupEditor": "terminal",
    "cSpell.enabled": false,
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "zig.path": "zig",
    "editor.fontFamily": " Menlo, Monaco, 'Courier New', monospace"

